tore.dispatch('LogOut').then(() => {\r\n              Message.error(err)\r\n              next({ path: '/' })\r\n            })\r\n          })\r\n      } else {\r\n        next()\r\n      }\r\n    }\r\n  } else {\r\n    // 没有token\r\n    if (whiteList.indexOf(to.path) !== -1) {\r\n      // 在免登录白名单，直接进入\r\n      next()\r\n    } else {\r\n      next(`/login?redirect=${to.fullPath}`) // 否则全部重定向到登录页\r\n      NProgress.done()\r\n    }\r\n  }\r\n})\r\n\r\nrouter.afterEach(() => {\r\n  NProgress.done()\r\n})\r\n"]}]}
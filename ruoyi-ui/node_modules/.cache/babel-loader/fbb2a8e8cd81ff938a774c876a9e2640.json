  yAxis: [{\r\n          type: 'value',\r\n          axisTick: {\r\n            show: false\r\n          }\r\n        }],\r\n        series: [{\r\n          name: 'pageA',\r\n          type: 'bar',\r\n          stack: 'vistors',\r\n          barWidth: '60%',\r\n          data: [79, 52, 200, 334, 390, 330, 220],\r\n          animationDuration\r\n        }, {\r\n          name: 'pageB',\r\n          type: 'bar',\r\n          stack: 'vistors',\r\n          barWidth: '60%',\r\n          data: [80, 52, 200, 334, 390, 330, 220],\r\n          animationDuration\r\n        }, {\r\n          name: 'pageC',\r\n          type: 'bar',\r\n          stack: 'vistors',\r\n          barWidth: '60%',\r\n          data: [30, 52, 200, 334, 390, 330, 220],\r\n          animationDuration\r\n        }]\r\n      })\r\n    }\r\n  }\r\n}\r\n</script>\r\n"]}]}
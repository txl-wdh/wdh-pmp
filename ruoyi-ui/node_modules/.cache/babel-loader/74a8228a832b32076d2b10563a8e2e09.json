b,EAAE,CAACM,iBAApB;AACAO,YAAAA,QAAQ,IAAIA,QAAQ,CAACD,CAAD,CAApB;AACD,WAHD;AAIAZ,UAAAA,EAAE,CAACc,WAAH,GAAiBP,SAAjB;AACD;AArBH;AAuBD,GAzBY;AA0BbQ,EAAAA,MA1Ba,kBA0BNf,EA1BM,EA0BFC,OA1BE,EA0BO;AAClB,QAAIA,OAAO,CAACE,GAAR,KAAgB,SAApB,EAA+B;AAC7BH,MAAAA,EAAE,CAACI,mBAAH,GAAyBH,OAAO,CAACI,KAAjC;AACD,KAFD,MAEO,IAAIJ,OAAO,CAACE,GAAR,KAAgB,OAApB,EAA6B;AAClCH,MAAAA,EAAE,CAACM,iBAAH,GAAuBL,OAAO,CAACI,KAA/B;AACD,KAFM,MAEA;AACLL,MAAAA,EAAE,CAACc,WAAH,CAAeL,IAAf,GAAsB,YAAY;AAAE,eAAOR,OAAO,CAACI,KAAf;AAAuB,OAA3D;;AACAL,MAAAA,EAAE,CAACc,WAAH,CAAeJ,MAAf,GAAwB;AAAA,eAAMT,OAAO,CAACE,GAAR,KAAgB,KAAhB,GAAwB,KAAxB,GAAgC,MAAtC;AAAA,OAAxB;AACD;AACF,GAnCY;AAoCba,EAAAA,MApCa,kBAoCNhB,EApCM,EAoCFC,OApCE,EAoCO;AAClB,QAAI,CAACD,EAAE,CAACiB,WAAR,EAAqB;;AACrB,QAAIhB,OAAO,CAACE,GAAR,KAAgB,SAApB,EAA+B;AAC7B,aAAOH,EAAE,CAACI,mBAAV;AACD,KAFD,MAEO,IAAIH,OAAO,CAACE,GAAR,KAAgB,OAApB,EAA6B;AAClC,aAAOH,EAAE,CAACM,iBAAV;AACD,KAFM,MAEA;AACLN,MAAAA,EAAE,CAACc,WAAH,CAAeI,OAAf;;AACA,aAAOlB,EAAE,CAACc,WAAV;AACD;AACF;AA9CY,C","sourcesContent":["/**\r\n* v-clipboard 文字复制剪贴\r\n* Copyright (c) 2021 ruoyi\r\n*/\r\n\r\nimport Clipboard from 'clipboard'\r\nexport default {\r\n  bind(el, binding, vnode) {\r\n    switch (binding.arg) {\r\n      case 'success':\r\n        el._vClipBoard_success = binding.value;\r\n        break;\r\n      case 'error':\r\n        el._vClipBoard_error = binding.value;\r\n        break;\r\n      default: {\r\n        const clipboard = new Clipboard(el, {\r\n          text: () => binding.value,\r\n          action: () => binding.arg === 'cut' ? 'cut' : 'copy'\r\n        });\r\n        clipboard.on('success', e => {\r\n          const callback = el._vClipBoard_success;\r\n          callback && callback(e);\r\n        });\r\n        clipboard.on('error', e => {\r\n          const callback = el._vClipBoard_error;\r\n          callback && callback(e);\r\n        });\r\n        el._vClipBoard = clipboard;\r\n      }\r\n    }\r\n  },\r\n  update(el, binding) {\r\n    if (binding.arg === 'success') {\r\n      el._vClipBoard_success = binding.value;\r\n    } else if (binding.arg === 'error') {\r\n      el._vClipBoard_error = binding.value;\r\n    } else {\r\n      el._vClipBoard.text = function () { return binding.value; };\r\n      el._vClipBoard.action = () => binding.arg === 'cut' ? 'cut' : 'copy';\r\n    }\r\n  },\r\n  unbind(el, binding) {\r\n    if (!el._vClipboard) return\r\n    if (binding.arg === 'success') {\r\n      delete el._vClipBoard_success;\r\n    } else if (binding.arg === 'error') {\r\n      delete el._vClipBoard_error;\r\n    } else {\r\n      el._vClipBoard.destroy();\r\n      delete el._vClipBoard;\r\n    }\r\n  }\r\n}\r\n"]}]}